{
  "manifest_version": 2,
  "name": "KaiWeb",
  "description": "The interweb, kaistyle",
  "version": "1.0",
  "author": "Kai Dalgleish",
  "homepage_url": "http://kaidalgleish.io",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "all_frames": true,
    "js": ["scripts/networkEliminator.js"]
  }]
}